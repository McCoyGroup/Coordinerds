 CellChangeTimes->{{3.792767659085104*^9, 3.792767684330935*^9}, {
   3.792768762958074*^9, 3.79276878644915*^9}, {3.792768823805715*^9, 
   3.792768825887085*^9}, {3.7927688636189413`*^9, 3.792768875629446*^9}, 
   3.79276927695639*^9, {3.7927741342628717`*^9, 
   3.7927741348790407`*^9}},ExpressionUUID->"47555812-a95f-4c40-87ec-\
4796308c8eb7"],

Cell["\<\
This is all well and good, but we can write this in a more intuitive way. \
Letting \
\>", "Text",
 CellChangeTimes->{{3.792769283360804*^9, 
  3.792769308143152*^9}},ExpressionUUID->"c7aa215c-e665-422a-a316-\
82caae49331a"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Del]", 
     StyleBox["X",
