     RowBox[{
      FractionBox["\[PartialD]", 
       RowBox[{"\[PartialD]", 
        SubscriptBox["x", "\[Alpha]"]}]], 
      SubscriptBox["r", "i"]}], 
     RowBox[{
      FractionBox["\[PartialD]", 
       RowBox[{"\[PartialD]", 
        SubscriptBox["x", "\[Alpha]"]}]], 
