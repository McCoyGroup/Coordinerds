Cell[TextData[{
 "Given a tensor ",
 StyleBox["A",
  FontWeight->"Bold"],
 ", and some variable ",
 StyleBox["x",
  FontSlant->"Italic"],
 " upon which the elements of ",
 StyleBox["A",
  FontWeight->"Bold"],
 " may depend we'll define the derivative element-wise such that"
}], "ExpositoryText",
