Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Del]", 
     RowBox[{"x", ",", "y"}]], "f"}], "=", 
   RowBox[{"(", GridBox[{
      {GridBox[{
         {"", 
          RowBox[{
           FractionBox[
            SuperscriptBox["\[PartialD]", "2"], 
            RowBox[{
             RowBox[{"\[PartialD]", 
              SubscriptBox["x", "1"]}], 
             RowBox[{"\[PartialD]", 
              SubscriptBox["y", "1"]}]}]], "f"}]}
        }], 
       RowBox[{
        FractionBox[
         SuperscriptBox["\[PartialD]", "2"], 
         RowBox[{
          RowBox[{"\[PartialD]", 
           SubscriptBox["x", "1"]}], 
          RowBox[{"\[PartialD]", 
           SubscriptBox["y", "2"]}]}]], "f"}], "\[CenterEllipsis]", 
       RowBox[{
        FractionBox[
         SuperscriptBox["\[PartialD]", "2"], 
         RowBox[{
          RowBox[{"\[PartialD]", 
           SubscriptBox["x", "1"]}], 
          RowBox[{"\[PartialD]", 
           SubscriptBox["y", "m"]}]}]], "f"}]},
      {
       RowBox[{
        FractionBox[
         SuperscriptBox["\[PartialD]", "2"], 
         RowBox[{
          RowBox[{"\[PartialD]", 
           SubscriptBox["x", "2"]}], 
          RowBox[{"\[PartialD]", 
           SubscriptBox["y", "1"]}]}]], "f"}], " ", " ", 
       ItemBox["\[VerticalEllipsis]",
        Alignment->Center]},
      {
       ItemBox["\[VerticalEllipsis]",
        Alignment->Center], " ", "\[DescendingEllipsis]", " "},
      {
       RowBox[{
        FractionBox[
         SuperscriptBox["\[PartialD]", "2"], 
         RowBox[{
          RowBox[{"\[PartialD]", 
           SubscriptBox["x", "n"]}], 
          RowBox[{"\[PartialD]", 
           SubscriptBox["y", "1"]}]}]], "f"}], "\[CenterEllipsis]", " ", 
       RowBox[{
        FractionBox[
         SuperscriptBox["\[PartialD]", "2"], 
         RowBox[{
          RowBox[{"\[PartialD]", 
           SubscriptBox["x", "n"]}], 
          RowBox[{"\[PartialD]", 
           SubscriptBox["y", "m"]}]}]], "f"}]}
     }], ")"}]}], TraditionalForm]], "ExpositoryMathCentered",
