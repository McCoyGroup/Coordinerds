   RowBox[{
    SubscriptBox["\[Del]", "x"], "f"}], "=", 
   RowBox[{"(", 
    RowBox[{
     FractionBox["\[PartialD]", 
      RowBox[{"\[PartialD]", 
       SubscriptBox["x", "1"]}]], GridBox[{
