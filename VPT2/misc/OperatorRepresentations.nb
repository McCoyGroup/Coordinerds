        SubscriptBox["Q", "j"], 
        SubscriptBox["Q", "k"], 
        TemplateBox[{"m"},
         "Ket"]}]}]}]},
    {
     RowBox[{"=", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[Sum]", 
         RowBox[{"\[VeryThinSpace]", 
          RowBox[{"i", "\[VeryThinSpace]", "j", "\[VeryThinSpace]", "k"}]}]], 
        
        RowBox[{
         FractionBox["1", "2"], 
         FractionBox[
          RowBox[{"\[PartialD]", 
           SubscriptBox["g", 
            RowBox[{"\[VeryThinSpace]", 
             RowBox[{"i", "\[VeryThinSpace]", "j"}]}]]}], 
          RowBox[{"\[PartialD]", 
           SubscriptBox["Q", "k"]}]], 
         TemplateBox[{"n"},
          "Bra"], 
         SubscriptBox["p", "i"], 
         SubscriptBox["Q", "k"], 
         SubscriptBox["p", "j"], 
         TemplateBox[{"m"},
          "Ket"]}]}], " ", "+", " ", 
       RowBox[{
        FractionBox["1", "6"], 
        FractionBox[
         RowBox[{
          SuperscriptBox["\[PartialD]", "3"], "V"}], 
         RowBox[{
          RowBox[{"\[PartialD]", 
           SubscriptBox["Q", "i"]}], 
          SubscriptBox["Q", "j"], 
          SubscriptBox["Q", "k"]}]], 
        TemplateBox[{"n"},
         "Bra"], 
        SubscriptBox["Q", "i"], 
        SubscriptBox["Q", "j"], 
        SubscriptBox["Q", "k"], 
        TemplateBox[{"m"},
         "Ket"]}]}]}]}
   }], TraditionalForm]], "ExpositoryMathCentered",
 CellChangeTimes->{{3.793484956128211*^9, 3.79348497881885*^9}, {
  3.793485038676215*^9, 
  3.793485075426423*^9}},ExpressionUUID->"1d9ed4ea-023a-471b-8f42-\
d3021f686eb6"],

