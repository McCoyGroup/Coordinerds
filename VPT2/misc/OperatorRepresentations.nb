Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox[
    StyleBox["H",
     FontSlant->"Plain"], 
    RowBox[{"(", "1", ")"}]], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Sum]", 
      RowBox[{"\[VeryThinSpace]", 
       RowBox[{"i", "\[VeryThinSpace]", "j", "\[VeryThinSpace]", "k"}]}]], 
     RowBox[{
      FractionBox["1", "2"], 
      FractionBox[
       RowBox[{"\[PartialD]", 
        SubscriptBox["g", 
         RowBox[{"\[VeryThinSpace]", 
          RowBox[{"i", "\[VeryThinSpace]", "j"}]}]]}], 
       RowBox[{"\[PartialD]", 
        SubscriptBox["Q", "k"]}]], 
      SubscriptBox["p", "i"], 
      SubscriptBox["Q", "k"], 
      SubscriptBox["p", "j"]}]}], "+", 
    RowBox[{
     FractionBox["1", "6"], 
     FractionBox[
      RowBox[{
       SuperscriptBox["\[PartialD]", "3"], "V"}], 
      RowBox[{
       RowBox[{"\[PartialD]", 
        SubscriptBox["Q", "i"]}], 
       SubscriptBox["Q", "j"], 
       SubscriptBox["Q", "k"]}]], 
     SubscriptBox["Q", "i"], 
     SubscriptBox["Q", "j"], 
     SubscriptBox["Q", "k"]}]}]}], TraditionalForm]], "ExpositoryMathCentered",\

 CellChangeTimes->{{3.793484698745399*^9, 3.7934846987822123`*^9}, {
