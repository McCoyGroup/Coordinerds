        SubscriptBox["Q", "j"]}]}]}]},
    {
     RowBox[{
      RowBox[{
       SubscriptBox["p", "i"], 
       SubscriptBox["Q", "j"], 
       SubscriptBox["p", "i"]}], "=", 
      RowBox[{
       SubscriptBox["p", "i"], 
       SubscriptBox["p", "i"], 
       SubscriptBox["Q", "j"]}]}]}
   }], TraditionalForm]], "ExpositoryMathCentered",
