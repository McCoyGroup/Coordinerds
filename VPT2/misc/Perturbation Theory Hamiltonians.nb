Cell[TextData[{
 "Considering the terms we will actually need to compute, we will need \
expectation values of ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["H", 
    RowBox[{"(", "1", ")"}]], TraditionalForm]],ExpressionUUID->
  "9096134c-17e9-4c0b-ae23-b72055898636"],
 " across states, i.e."
}], "Text",
 CellChangeTimes->{{3.770833390879895*^9, 3.770833419760157*^9}, {
  3.7708334849828157`*^9, 3.7708335147269707`*^9}, {3.770833580038875*^9, 
  3.77083358030214*^9}},ExpressionUUID->"8f62180f-b014-44e9-b593-\
e168685efe00"],

Cell[BoxData[
 FormBox[GridBox[{
    {
     RowBox[{
      RowBox[{
       TemplateBox[{"n"},
        "Bra"], 
       SuperscriptBox["H", 
        RowBox[{"(", "1", ")"}]], 
       TemplateBox[{"m"},
        "Ket"]}], "=", 
      RowBox[{
       TemplateBox[{"n"},
        "Bra"], 
       FractionBox["1", "2"], 
       RowBox[{
        SubscriptBox["\[Sum]", 
         RowBox[{"i", ",", "j", ",", "k"}]], 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            FractionBox[
             RowBox[{"\[PartialD]", 
              SubscriptBox["g", 
               RowBox[{"i", "\[InvisibleSpace]", "j"}]]}], 
             RowBox[{"\[PartialD]", 
              SubscriptBox["Q", "k"]}]], 
            SubscriptBox["p", "i"], 
            SubscriptBox["Q", "k"], 
            SubscriptBox["p", "j"]}], "+", 
           RowBox[{
            FractionBox["1", "2"], 
            RowBox[{
             FractionBox[
              SuperscriptBox["\[PartialD]", "3"], 
              RowBox[{
               RowBox[{"\[PartialD]", 
                SubscriptBox["Q", "i"]}], 
               RowBox[{"\[PartialD]", 
                SubscriptBox["Q", "j"]}], 
               RowBox[{"\[PartialD]", 
                SubscriptBox["Q", "k"]}]}]], "V"}], 
            SubscriptBox["Q", "i"], 
            SubscriptBox["Q", "j"], 
            SubscriptBox["Q", "k"]}]}], ")"}], 
         TemplateBox[{"m"},
          "Ket"]}]}]}]}]},
    {
     RowBox[{"=", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{
        SubscriptBox["\[Sum]", 
         RowBox[{"i", ",", "j", ",", "k"}]], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           FractionBox[
            RowBox[{"\[PartialD]", 
             SubscriptBox["g", 
              RowBox[{"i", "\[InvisibleSpace]", "j"}]]}], 
            RowBox[{"\[PartialD]", 
             SubscriptBox["Q", "k"]}]], 
           TemplateBox[{"n"},
            "Bra"], 
           SubscriptBox["p", "i"], 
           SubscriptBox["Q", "k"], 
           SubscriptBox["p", "j"], 
           TemplateBox[{"m"},
            "Ket"]}], "+", 
          RowBox[{
           FractionBox["1", "2"], 
           RowBox[{
            FractionBox[
             SuperscriptBox["\[PartialD]", "3"], 
             RowBox[{
              RowBox[{"\[PartialD]", 
               SubscriptBox["Q", "i"]}], 
              RowBox[{"\[PartialD]", 
               SubscriptBox["Q", "j"]}], 
              RowBox[{"\[PartialD]", 
               SubscriptBox["Q", "k"]}]}]], "V"}], 
           TemplateBox[{"n"},
            "Bra"], 
           SubscriptBox["Q", "i"], 
           SubscriptBox["Q", "j"], 
           SubscriptBox["Q", "k"], 
           TemplateBox[{"m"},
            "Ket"]}]}], ")"}]}]}]}]}
   }], TraditionalForm]], "ExpositoryMathCentered",
 CellChangeTimes->{{3.770833512113696*^9, 3.7708335724462442`*^9}, {
