Cell[BoxData[
 RowBox[{
  RowBox[{"makeMat", "[", 
   RowBox[{"coeffs_", ",", " ", "dimensions_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"ArrayFlatten", "@", "\[IndentingNewLine]", 
   RowBox[{"Table", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"coeffs", "[", 
       RowBox[{"[", "i", "]"}], "]"}], "*", 
      RowBox[{"coeffs", "[", 
       RowBox[{"[", "j", "]"}], "]"}], "*", "\[IndentingNewLine]", 
      RowBox[{"Fold", "[", "\[IndentingNewLine]", 
       RowBox[{"KroneckerProduct", ",", "\[IndentingNewLine]", 
        RowBox[{"MapIndexed", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"Which", "[", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"#2", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "\[Equal]", "i"}], "&&", 
              RowBox[{"i", "\[Equal]", "j"}]}], ",", "\[IndentingNewLine]", 
             RowBox[{"MatrixPower", "[", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"aDgMat", "[", "#", "]"}], " ", "-", " ", 
                 RowBox[{"aMat", "[", "#", "]"}]}], ")"}], ",", 
               "\[IndentingNewLine]", "2"}], "\[IndentingNewLine]", "]"}], 
             ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{
               RowBox[{"#2", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "\[Equal]", "i"}], "||", 
              RowBox[{
               RowBox[{"#2", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "\[Equal]", "j"}]}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"aDgMat", "[", "#", "]"}], " ", "-", " ", 
              RowBox[{"aMat", "[", "#", "]"}]}], ",", "\[IndentingNewLine]", 
             "True", ",", "\[IndentingNewLine]", 
             RowBox[{"IdentityMatrix", "[", 
              RowBox[{"#", ",", " ", "SparseArray"}], "]"}]}], 
            "\[IndentingNewLine]", "]"}], "&"}], ",", "\[IndentingNewLine]", 
          "dimensions"}], 
         RowBox[{"(*", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"i", "\[Equal]", "j"}], ",", " ", "dimensions", ",", " ", 
            
            RowBox[{"Delete", "[", 
             RowBox[{"dimensions", ",", " ", "j"}], "]"}]}], "]"}], "*)"}], 
         "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"i", ",", " ", 
       RowBox[{"Range", "[", 
        RowBox[{"Length", "@", "dimensions"}], "]"}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"j", ",", " ", 
       RowBox[{"Range", "[", 
        RowBox[{"Length", "@", "dimensions"}], "]"}]}], "}"}]}], 
    "\[IndentingNewLine]", "]"}]}]}]], "Input",
