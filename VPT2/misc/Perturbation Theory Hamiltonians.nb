Cell[CellGroupData[{

Cell["Full Hamiltonian", "Subsection",
 CellChangeTimes->{{3.770830930189588*^9, 3.770830938452623*^9}, {
  3.770833230058065*^9, 
  3.770833233881839*^9}},ExpressionUUID->"18332b3d-7d7b-4227-9f3e-\
dd9a92c7edd3"],

Cell[TextData[{
 "Our actual PT Hamiltonian has three bits to it (note that ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["H", 
    RowBox[{"(", "0", ")"}]], TraditionalForm]],ExpressionUUID->
  "701a31e9-a837-4dbd-a8ec-616f821a7a4b"],
 " is properly 2D but all of the off-diagonal terms are zero)"
}], "Text",
 CellChangeTimes->{{3.770830951940905*^9, 3.770830974542657*^9}, {
  3.770831005580039*^9, 3.770831008396003*^9}, {3.770833331849421*^9, 
  3.770833332280239*^9}, {3.770833362480253*^9, 
  3.770833387055225*^9}},ExpressionUUID->"7c0a6ffc-dbd9-4649-b07a-\
257db5fac44f"],

Cell[BoxData[
 FormBox[GridBox[{
    {
     RowBox[{
      SuperscriptBox["H", 
       RowBox[{"(", "0", ")"}]], " ", "=", " ", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{
        SubscriptBox["\[Sum]", "i"], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           SubscriptBox["g", 
            RowBox[{"i", "\[InvisibleSpace]", "i"}]], 
           SubscriptBox["p", "i"], 
           SubscriptBox["p", "i"]}], "+", 
          RowBox[{
           RowBox[{
            FractionBox[
             SuperscriptBox["\[PartialD]", "2"], 
             RowBox[{"\[PartialD]", 
              SuperscriptBox[
               SubscriptBox["Q", "i"], "2"]}]], "V"}], "\[VeryThinSpace]", 
           SubscriptBox["Q", "i"], 
           SubscriptBox["Q", "i"]}]}], ")"}]}]}]}]},
    {
     RowBox[{
      SuperscriptBox["H", 
       RowBox[{"(", "1", ")"}]], "=", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{
        SubscriptBox["\[Sum]", 
         RowBox[{"i", ",", "j", ",", "k"}]], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           FractionBox[
            RowBox[{"\[PartialD]", 
             SubscriptBox["g", 
              RowBox[{"i", "\[InvisibleSpace]", "j"}]]}], 
            RowBox[{"\[PartialD]", 
             SubscriptBox["Q", "k"]}]], 
           SubscriptBox["p", "i"], 
           SubscriptBox["Q", "k"], 
           SubscriptBox["p", "j"]}], "+", 
          RowBox[{
           FractionBox["1", "3"], 
           RowBox[{
            FractionBox[
             SuperscriptBox["\[PartialD]", "3"], 
             RowBox[{
              RowBox[{"\[PartialD]", 
               SubscriptBox["Q", "i"]}], 
              RowBox[{"\[PartialD]", 
               SubscriptBox["Q", "j"]}], 
              RowBox[{"\[PartialD]", 
               SubscriptBox["Q", "k"]}]}]], "V"}], 
           SubscriptBox["Q", "i"], 
           SubscriptBox["Q", "j"], 
           SubscriptBox["Q", "k"]}]}], ")"}]}]}]}]},
    {
     RowBox[{
      SuperscriptBox["H", 
       RowBox[{"(", "2", ")"}]], "=", 
      RowBox[{
       FractionBox["1", "4"], 
       RowBox[{
        SubscriptBox["\[Sum]", 
         RowBox[{"i", ",", "j", ",", "kl"}]], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           FractionBox[
            RowBox[{
             SuperscriptBox["\[PartialD]", "2"], 
             SubscriptBox["g", 
              RowBox[{"i", "\[InvisibleSpace]", "j"}]]}], 
            RowBox[{
             RowBox[{"\[PartialD]", 
              SubscriptBox["Q", "k"]}], 
             RowBox[{"\[PartialD]", 
              SubscriptBox["Q", "j"]}]}]], 
           SubscriptBox["p", "i"], 
           SubscriptBox["Q", "k"], 
           SubscriptBox["Q", "l"], 
           SubscriptBox["p", "j"]}], "+", 
          RowBox[{
           FractionBox["1", "6"], 
           RowBox[{
            FractionBox[
             SuperscriptBox["\[PartialD]", "3"], 
             RowBox[{
              RowBox[{"\[PartialD]", 
               SubscriptBox["Q", "i"]}], 
              RowBox[{"\[PartialD]", 
               SubscriptBox["Q", "j"]}], 
              RowBox[{"\[PartialD]", 
               SubscriptBox["Q", "k"]}], 
              RowBox[{"\[PartialD]", 
               SubscriptBox["Q", "l"]}]}]], "V"}], 
           SubscriptBox["Q", "i"], 
           SubscriptBox["Q", "j"], 
           SubscriptBox["Q", "k"], 
           SubscriptBox["Q", "l"]}]}], ")"}]}]}]}]}
   }], TraditionalForm]], "ExpositoryMathCentered",
 CellChangeTimes->{{3.7708310146849127`*^9, 3.770831126000095*^9}, {
   3.77083202980801*^9, 3.770832132082119*^9}, {3.770832168368641*^9, 
